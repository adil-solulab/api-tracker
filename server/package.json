  },
  "author": "Adil Ansari",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cp": "^0.2.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "helmet": "^6.0.0",
    "joi": "^17.13.3",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.30.1",
    "mongoose": "^6.13.0",
    "mustache": "^4.2.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "unique-names-generator": "^4.7.1",
    "winston": "^3.8.2"
