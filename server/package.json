  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.189",
    "@types/morgan": "^1.9.3",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.0.3"
