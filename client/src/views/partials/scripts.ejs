<script>
	const Logout = async () => {
		try {
			const request = axiosConfig(constant.URLS.LOGOUT, 'POST', {})
			const logout = await MakeAPICall(request)
			if (logout) {
				await MakeAPICall({
					method: 'POST',
					url: '/destroy-session',
				})
				storage.clear()
			}
		} catch (error) {
			ToastMsg(error, 'Error')
		} finally {
			window.location.href = '/'
		}
	}
</script>
