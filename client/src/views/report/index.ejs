<div style="max-width: 100%;" class="content-wrapper">
	<!-- Content -->

	<div style="padding-left: 0; padding-right: 0; max-width: 98%;" class="container-xxl flex-grow-1 container-p-y">
		<!-- Users List Table -->
		<div class="card">
			<h5 class="card-header">API Hit Reports</h5>
			<div class="card-body">
				<form id="SearchLeadForm" class="dt_adv_search" onsubmit="return false">
					<div class="row">
						<div class="col-12">
							<div class="row g-3">
								<div class="col-12 col-sm-6 col-lg-3">
									<div class="form-floating form-floating-outline mb-2">
										<select data-allow-clear="true" id="CompanyCode" class="select2 form-select" name="brandinput">
											<option value="">Select Company</option>
										</select>
										<label for="user-role">Company Code</label>
									</div>
								</div>
							
								
								<div class="col-12 col-sm-6 col-lg-3 date">
									<div class="form-floating form-floating-outline mb-2">
										<input placeholder="DD/MM/YYYY" type="text" class="form-control" id="LeadDateFrom" />
										<label>From Date</label>
									</div>
								</div>
								<div class="col-12 col-sm-6 col-lg-3 date">
									<div class="form-floating form-floating-outline mb-2">
										<input placeholder="DD/MM/YYYY" type="text" class="form-control" id="LeadDateTo" />
										<label>To Date</label>
									</div>
								</div>
								
								
                                
                               
                                <div class="col-12 col-sm-6 col-lg-3">
									<div class="form-floating form-floating-outline">
										<button id="SearchLeadBtn" type="submit" class="btn btn-lg btn-primary">Search</button>
									</div>
								</div>
							</div>
						</div>
					</div>
				</form>
			</div>
			<hr class="mt-0" />
			<div class="card-datatable table-responsive">
				<table id="Datatable" class="datatables-users table">
					<thead class="table-light">
						<tr class="text-black">
							<th>Company Code</th>
							<th>Total Hits</th>
							<th>Average Hits/sec</th>
							<th>date</th>
							<th>Action</th>
						</tr>
					</thead>
				</table>
			</div>
		</div>
	</div>

	<!-- / Content -->

	<!-- Footer -->
	<%- include('../partials/footer') %>
	<!-- / Footer -->

	<div class="content-backdrop fade"></div>
</div>
<script src="js/core/datatable-helper.js"></script>
<script src="js/reports/index.js"></script>
